	<queryString>
		<![CDATA[SELECT
    d.anexo,
	ta.descricao as tipo,
	dst.destino,
	to_char(d.data_recebimento, 'DD/MM/YYYY') as dt_recebimento
FROM
    documentos d
Left Join tipo_anexo ta on ta.id = d.fk_tipo_anexo
